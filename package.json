{
  "name": "react-searchable",
  "version": "0.1.1",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "repository": "https://github.com/prograsdk/react-searchable",
  "author": "Emil BÃ¦kdahl <emilsbaekdahl@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.3",
    "@types/lodash.debounce": "^4.0.4",
    "@types/react": "^16.4.15",
    "codecov": "^3.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "ts-jest": "^23.10.3",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.1"
  },
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "lint": "./node_modules/tslint/bin/tslint -c ./tslint.json 'src/**/*.ts'",
    "test": "./node_modules/jest/bin/jest.js",
    "docs": "./node_modules/typedoc/bin/typedoc --out ./docs/ --exclude '**/__tests__/*' ./src/ && touch docs/.nojekyll",
    "codecov": "./node_modules/codecov/bin/codecov"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTest.js",
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/index.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8"
  }
}
